{
  "manifest_version": 3,
  "name": "TweetCode",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": { 
      "default_popup": "hello.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content.js"]
    }
  ]
}
